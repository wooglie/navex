{
  "manifest_version": 3,
  "name": "Navex",
  "version": "1.0.2",
  "description": "Navigate the web faster with custom @ shortcuts",
  "author": "Ivan Maga≈° @ 800",
  "homepage_url": "https://800.hr",
  "permissions": ["storage", "activeTab", "notifications"],
  "host_permissions": ["https://www.google.com/*", "https://*.gstatic.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "omnibox": {
    "keyword": "@"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
